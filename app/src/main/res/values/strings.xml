    <string name="tongue_4">הורדת לשון</string>
    <string name="tongue_4_task">הוצא לשון למטה, לכיוון הסנטר למשך 10 שניות.</string>
    <string name="tongue_6">ליקוק שפתיים</string>
    <string name="tongue_6_task">לקק את השפה התחתונה/עליונה/בתנועה סיבובית מצד לצד מספר פעמים. הקפד על הגעת הלשון לזווית הפה. </string>
    <string name="tongue_7">הזזת לשון מצד לצד</string>
    <string name="tongue_7_task">הזז את הלשון מזווית פה ימין לזווית פה שמאל לסירוגין, מבלי להישען על השפתיים.</string>
    <string name="tongue_8">הזזת לשון במהירות</string>
    <string name="tongue_8_task">הזז את הלשון מצד לצד מחוץ לפה, כך שהלשון כל פעם נוגעת בזווית הפה. בצע שוב, במהירות.</string>
    <string name="tongue_9">הזזת לשון מלחי ללחי</string>
    <string name="tongue_9_task">העבר את הלשון בתוך הפה מלחי ימין ללחי שמאל לסירוגין, ללא מגע עם השיניים.</string>
    <string name="tongue_10">דחיפת שפדל</string>
    <string name="tongue_10_task">הוצא את הלשון קדימה וגע בשפדל. דחוף את השפדל לכיוון הפה ועם הלשון התנגד. אל תתן לשפדל להיכנס פנימה.</string>
    <string name="tongue_11">דחיפת לחיים</string>
    <string name="tongue_11_task">דחוף את הלחיים עם קצה הלשון. התנגד לדחיפה עם היד מבחוץ.</string>
    <string name="tongue_12">דחיפת שפדל מעלה</string>
    <string name="tongue_12_task">לפתוח את הפה ולהניח שפדל על הלשון. לנסות להרים לשון למעלה, בעוד השפדל מתנגד ודוחף לשון לרצפת הפה.</string>
    <string name="tongue_16">נקישה עם הלשון</string>
    <string name="tongue_16_task">"נקישה" עם הלשון קצת אחורנית.</string>
    <string name="tongue_24">בליעת רוק</string>
    <string name="tongue_24_task">שים את הלשון בין השיניים. בצע בליעה של רוק כאשר הלשון נשארת בין השיניים. </string>

